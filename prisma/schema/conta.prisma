
model Conta {
  id Int @id @default(autoincrement())
  email String @unique
  cpf String @unique
  nome String
  tema Tema @default(Light)
  displayName String @default("")
  customWpp String @default("")

  // ORM Relations
  federado Federados?
  carteira Carteira?
}

enum Tema {
  Dark
  Light
}
