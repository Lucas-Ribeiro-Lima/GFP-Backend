{
  "name": "gfp-backend",
  "version": "1.0.1",
  "description": "GFP backend express API server",
  "main": "main.ts",
  "type": "module",
  "scripts": {
    "start": "yarn db:push && node ./dist/main.js",
    "dev": "npx nodemon --exec node --inspect --loader ts-node/esm --env-file=.env ./src/main.ts",
    "build": "npx tsup src/**/*.ts --format esm,cjs --target ESNext --splitting --sourcemap",
    "test": "npx vitest",
    "db:push": "npx prisma db push"
  },
  "author": "Lucas Ribeiro Lima",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.22.0",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.7.4",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-oauth2": "^1.4.17",
    "@types/redis": "^4.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/winston": "^2.4.4",
    "eslint": "^9.11.1",
    "globals": "^15.10.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.7.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.2"
  }
}
